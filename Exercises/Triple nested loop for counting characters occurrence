int count_chars(const char *characters, FILE *stream) {
    int total = 0;
    char line[100];
    while (fgets(line, 100, stream) != NULL) {
        for (int j = 0; j < (int)strlen(characters); j++) {
            for (int i = 0; i < (int)strlen(line); i++) {
                if(characters[j] == line[i]) {
                    total++;
                }
            }
        }
    }
    return total;
}
